{
  "name": "@sanity/cli-core",
  "version": "0.1.0-alpha.4",
  "description": "Sanity CLI core package",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "cli",
    "tool"
  ],
  "homepage": "https://github.com/sanity-io/cli",
  "bugs": "https://github.com/sanity-io/cli/issues",
  "repository": "sanity-io/cli",
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./tree": {
      "source": "./src/_exports/tree.ts",
      "require": "./dist/_exports/tree.js",
      "import": "./dist/_exports/tree.js"
    },
    "./ux": {
      "source": "./src/_exports/ux.ts",
      "require": "./dist/_exports/ux.js",
      "import": "./dist/_exports/ux.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "@oclif/core": "^4.8.0",
    "@sanity/client": "^7.11.2",
    "@sanity/types": "^4.21.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "chalk": "^5.6.2",
    "configstore": "^7.0.0",
    "debug": "^4.4.3",
    "get-tsconfig": "^4.10.1",
    "import-meta-resolve": "^4.1.0",
    "jsdom": "^26.0.0",
    "json-lexer": "^1.2.0",
    "log-symbols": "^7.0.1",
    "ora": "^9.0.0",
    "tsx": "^4.20.3",
    "vite": "^7.1.6",
    "vite-node": "^3.0.8",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.14.0",
    "@types/debug": "^4.1.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.24",
    "eslint": "^9.39.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@sanity/eslint-config-cli": "0.0.0-alpha.1",
    "@repo/tsconfig": "3.70.0"
  },
  "engines": {
    "node": ">=20.19.1 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ src",
    "build:types": "tsc --project tsconfig.lib.json",
    "check:types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "posttest": "pnpm run lint",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ --watch src"
  }
}