{
  "name": "@sanity/cli-core",
  "version": "0.1.0-alpha.8",
  "description": "Sanity CLI core package",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "cli",
    "tool"
  ],
  "homepage": "https://github.com/sanity-io/cli",
  "bugs": "https://github.com/sanity-io/cli/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/cli.git",
    "directory": "packages/@sanity/cli-core"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./tree": {
      "source": "./src/_exports/tree.ts",
      "require": "./dist/_exports/tree.js",
      "import": "./dist/_exports/tree.js"
    },
    "./ux": {
      "source": "./src/_exports/ux.ts",
      "require": "./dist/_exports/ux.js",
      "import": "./dist/_exports/ux.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "@oclif/core": "^4.8.0",
    "@sanity/client": "^7.14.0",
    "@sanity/types": "^5.6.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "chalk": "^5.6.2",
    "configstore": "^7.0.0",
    "debug": "^4.4.3",
    "get-tsconfig": "^4.13.0",
    "import-meta-resolve": "^4.1.0",
    "jsdom": "^27.4.0",
    "json-lexer": "^1.2.0",
    "log-symbols": "^7.0.1",
    "ora": "^9.0.0",
    "tsx": "^4.21.0",
    "vite": "^7.3.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@sanity/codegen": "^5.6.0",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.10",
    "@types/debug": "^4.1.12",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.19.30",
    "eslint": "^9.39.2",
    "publint": "^0.3.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "@repo/tsconfig": "3.70.0",
    "@sanity/eslint-config-cli": "0.0.0-alpha.1"
  },
  "engines": {
    "node": ">=20.19.1 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ src --ignore '**/*.test.ts'",
    "build:types": "tsc --project tsconfig.lib.json",
    "check:types": "tsc --noEmit",
    "lint": "eslint .",
    "publint": "publint",
    "test": "vitest run",
    "posttest": "pnpm run lint",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ --watch src"
  }
}